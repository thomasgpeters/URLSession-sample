void local fn BuildSubViewsnibview _welcomeView, 		   @"WelcomeView",,  				( _objectListWidth + (_controlSpacer*2), _objectListHeight - _welcomeViewHeight )nibview _projectCreateView,	 @"ProjectCreateView",,		( _objectListWidth + (_controlSpacer*2), _objectListHeight - _projectCreateViewHeight )nibview _objectCreateView,	 @"ObjectCreateView",	,		( _objectListWidth + (_controlSpacer*2), _objectListHeight - _objectCreateViewHeight )// Init Toolbar and Viewsnibtoolbar _mainToolbar, @"Toolbar"WindowSetToolbar( _window, _mainToolbar )' set hiddenViewSetHidden( _objectListView, YES )ViewSetHidden( _welcomeView, NO )ViewSetHidden( _projectCreateView, YES )ViewSetHidden( _objectCreateView, YES )ViewSetAcceptsFirstResponder( _welcomeView, YES )end fnvoid local fn BuildProjectMainWindow'~'1// setup windows from xibnibwindow _window, @"MainWindow"fn TableViewSetCustomCellCallback( _objectListView, @[ID_KEY,OBJECT_KEY], @fn MyCustomCellCallback, NULL )fn TableViewSizeLastColumnToFit( _objectListView )// setup views from xibWindowCenter( _window )' WindowMakeMain( _projectMainWindow )ViewSetAutoresizesSubviews( _window, YES )end fn