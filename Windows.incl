void local fn BuildOldWindow'~'1CGRect r = fn CGRectMake( 0, 0, 750, 550 )window 10, @"Hello URL", r, NSWindowStyleMaskTitled + NSWindowStyleMaskClosabler = fn CGRectMake( 20, 60, 710, 470 )scrollview _scrollView, r, NSLineBorder,,10ViewSetAutoresizingMask( _scrollView, NSViewWidthSizable + NSViewHeightSizable )subclass textview _textView, r, _scrollView,, 10TextViewSetTextContainerInset( _textView, fn CGSizeMake( 8, 8 ) )TextViewSetBackgroundColor(    _textView, fn ColorWhite )TextSetColor(                  _textView, fn ColorBlack )TextViewAppendString(          _textView, @"\nIt may take a second or two to connect after pressing button.\n\n" )r = fn CGRectMake( 14, 13, 150, 32 )button _myButton,,, @"Call REST endpoint", rr = fn CGRectMake( 167, 13, 150, 32 )button _xibButton,,, @"Open Window.xib", rr = fn CGRectMake( 320, 13, 150, 32 )button _loginButton,,, @"Call Login endpoint", rtoggleOn = fn ImageNamed( @"toggle-on.png" )ImageSetSize( toggleOn, fn CGSizeMake(50, 50) )toggleOff = fn ImageNamed( @"toggle-off.png" )ImageSetSize( toggleOff, fn CGSizeMake(50, 50) )r = fn CGRectMake( 680, 6, 50, 50 )button _securityToggleBtn,,,, rif (isSecurityOn)ImageSetSize( toggleOn, fn CGSizeMake(50, 50) )ButtonSetImage( _securityToggleBtn, toggleOn )elseImageSetSize( toggleOff, fn CGSizeMake(50, 50) )ButtonSetImage( _securityToggleBtn, toggleOff )end ifend fnvoid local fn BuildSubViewsnibview _welcomeView, 						@"WelcomeView", 			@"Welcome View",  					( _objectListWidth + (_controlSpacer*2), _objectListHeight - _welcomeViewHeight )nibview _projectCreateView,				@"ProjectCreateView",	@"Project Create View",		( _objectListWidth + (_controlSpacer*2), _objectListHeight - _projectCreateViewHeight )nibview _objectCreateView,				@"ObjectCreateView",		@"Object Create View",			( _objectListWidth + (_controlSpacer*2), _objectListHeight - _objectCreateViewHeight )// Init Toolbar and Viewsnibtoolbar _mainToolbar, @"Toolbar"WindowSetToolbar( _window, _mainToolbar )ViewSetHidden( _objectListView, YES )' set hiddenViewSetHidden( _welcomeView, NO )ViewSetAcceptsFirstResponder( _welcomeView, YES )ViewSetHidden( _projectCreateView, YES )ViewSetHidden( _objectCreateView, YES )end fnvoid local fn BuildProjectMainWindow'~'1// setup windows from xibnibwindow _window, @"MainWindow"'fn TableViewSetCustomCellCallback( _contactListView, @[ID_KEY,CONTACT_KEY], @fn MyCustomCellCallback, NULL )'fn TableViewSizeLastColumnToFit( _contactListView )// setup views from xibWindowCenter( _window )' WindowMakeMain( _projectMainWindow )ViewSetAutoresizesSubviews( _window, YES )end fn