void local fn BuildWindow'~'1CGRect r = fn CGRectMake( 0, 0, 750, 550 )window _window, @"Hello URL", r, NSWindowStyleMaskTitled + NSWindowStyleMaskClosabler = fn CGRectMake( 20, 60, 710, 470 )scrollview _scrollView, r, NSLineBorder,,_windowViewSetAutoresizingMask( _scrollView, NSViewWidthSizable + NSViewHeightSizable )subclass textview _textView, r, _scrollView,, _windowTextViewSetTextContainerInset( _textView, fn CGSizeMake( 8, 8 ) )TextViewSetBackgroundColor(    _textView, fn ColorWhite )TextSetColor(                  _textView, fn ColorBlack )TextViewAppendString(          _textView, @"\nIt may take a second or two to connect after pressing button.\n\n" )r = fn CGRectMake( 14, 13, 150, 32 )button _myButton,,, @"Call REST endpoint", rr = fn CGRectMake( 167, 13, 150, 32 )button _xibButton,,, @"Open Window.xib", rr = fn CGRectMake( 320, 13, 150, 32 )button _loginButton,,, @"Call Login endpoint", rtoggleOn = fn ImageNamed( @"toggle-on.png" )ImageSetSize( toggleOn, fn CGSizeMake(50, 50) )toggleOff = fn ImageNamed( @"toggle-off.png" )ImageSetSize( toggleOff, fn CGSizeMake(50, 50) )r = fn CGRectMake( 680, 6, 50, 50 )button _securityToggleBtn,,,, rif (isSecurityOn)ImageSetSize( toggleOn, fn CGSizeMake(50, 50) )ButtonSetImage( _securityToggleBtn, toggleOn )elseImageSetSize( toggleOff, fn CGSizeMake(50, 50) )ButtonSetImage( _securityToggleBtn, toggleOff )end ifend fn