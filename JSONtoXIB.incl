local fn OpenXibFile as CFURLRef'~'1CFURLRef url = openpanel 1, @"Open an xib file", @"xib;xml"if ( ( url != Null ) )return urlend ifend fnlocal fn CreateNewXibDocument'~'1CFURLRef urlXMLDocumentRef xibDocumentNSXMLNodeOptions optionsErrorRef xibErrCFURLRef url = fn OpenXibFileif ( ( url != Null ) )xibDocument = fn XMLDocumentWithContentsOfURL(url, options, xibErr )end ifend fn